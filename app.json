{
  "expo": {
    "name": "tureluur-app",
    "slug": "tureluur-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-background.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo-background.png",
      "resizeMode": "contain",
      "backgroundColor": "#d7ded8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-background.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
